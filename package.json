{
  "name": "base-target",
  "description": "Plugin that adds support for defining declarative `target` configurations that directly map to functional tasks.",
  "version": "0.1.0",
  "homepage": "https://github.com/node-base/base-target",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "node-base/base-target",
  "bugs": {
    "url": "https://github.com/node-base/base-target/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "LICENSE",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "expand-target": "^0.6.4",
    "is-valid-app": "^0.2.0",
    "isobject": "^2.1.0",
    "kind-of": "^3.0.3",
    "lazy-cache": "^2.0.1",
    "mixin-deep": "^1.1.3"
  },
  "devDependencies": {
    "async-each-series": "^1.1.0",
    "base-app": "^0.2.6",
    "base-files-each": "^0.1.5",
    "extend-shallow": "^2.0.1",
    "gulp": "^3.9.1",
    "gulp-eslint": "^3.0.1",
    "gulp-format-md": "^0.1.9",
    "gulp-istanbul": "^1.0.0",
    "gulp-middleware": "^0.1.0",
    "gulp-mocha": "^2.2.0",
    "gulp-unused": "^0.1.2",
    "merge-stream": "^1.0.0",
    "mocha": "^2.5.3"
  },
  "keywords": [
    "api",
    "app",
    "application",
    "base",
    "baseplugin",
    "building-blocks",
    "create",
    "framework",
    "plugin",
    "plugins",
    "target",
    "tool",
    "toolkit",
    "tools"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "highlight": "expand-target",
      "apps": [
        "assemble",
        "verb",
        "update",
        "generate"
      ],
      "list": [
        "base-scaffold",
        "base-boilerplate"
      ]
    },
    "reflinks": [
      "base",
      "expand-target",
      "generate",
      "verb",
      "verb-readme-generator",
      "gulp",
      "verb-generate-readme"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
