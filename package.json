{
  "name": "base-target",
  "description": "Plugin that adds support for defining declarative `target` configurations that directly map to functional tasks.",
  "version": "0.1.0",
  "homepage": "https://github.com/node-base/base-target",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "node-base/base-target",
  "bugs": {
    "url": "https://github.com/node-base/base-target/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "LICENSE",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "async-each-series": "^1.1.0",
    "base-files-each": "^0.1.3",
    "expand-target": "^0.6.4",
    "extend-shallow": "^2.0.1",
    "is-scaffold": "^0.1.1",
    "is-valid-app": "^0.1.2",
    "isobject": "^2.1.0",
    "kind-of": "^3.0.3",
    "lazy-cache": "^2.0.1",
    "mixin-deep": "^1.1.3"
  },
  "devDependencies": {
    "base-app": "^0.2.6",
    "for-own": "^0.1.4",
    "gulp-format-md": "^0.1.9",
    "mocha": "^2.5.3"
  },
  "keywords": [
    "api",
    "app",
    "application",
    "base",
    "baseplugin",
    "building-blocks",
    "create",
    "framework",
    "plugin",
    "plugins",
    "target",
    "tool",
    "toolkit",
    "tools"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "reflinks": [
      "verb",
      "verb-readme-generator"
    ],
    "lint": {
      "reflinks": true
    }
  }
